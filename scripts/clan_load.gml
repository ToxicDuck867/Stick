/*clan data is stored in a series of parallel lists. It is importantthat all indexes stay in proper order relative to eachother.Different chunks of data (members, points, etc.) are seperated by labledlines. The labels merely serve as comments*/var _fileName, _file, _curMemberList, i, i2;global.clanNames=ds_list_create();global.clanLeaders=ds_list_create();global.clanPoints=ds_list_create();global.clanMemberLists=ds_list_create();_fileName=file_find_first("clans\*.cln",false);while (_fileName!=""){  ds_list_add(global.clanNames,string_copy(_fileName,0,string_length(_fileName)-4));  _file=file_text_open_read("clans\"+_fileName);  file_text_readln(_file);   ds_list_add(global.clanLeaders,file_text_read_string(_file));   file_text_readln(_file);  file_text_readln(_file);   ds_list_add(global.clanPoints,file_text_read_real(_file));  file_text_readln(_file);  file_text_readln(_file);     _curMemberList=ds_list_create();  while (!file_text_eof(_file))  {    ds_list_add(_curMemberList,string_lower(file_text_read_string(_file)));    file_text_readln(_file);  }  file_text_close(_file);  ds_list_add(global.clanMemberLists,_curMemberList);  _fileName=file_find_next();}global.clanMemberIndex=ds_list_create();global.clanNameIndex=ds_list_create();var _curMemberList;for (i=0; i<clan_count(); i+=1){    _curMemberList=ds_list_find_value(global.clanMemberLists,i);  for (i2=0; i2<ds_list_size(_curMemberList); i2+=1)  {    ds_list_add(global.clanMemberIndex,ds_list_find_value(_curMemberList,i2));    ds_list_add(global.clanNameIndex,ds_list_find_value(global.clanNames,i));  }    ds_list_add(global.clanMemberIndex,ds_list_find_value(global.clanLeaders,i));  ds_list_add(global.clanNameIndex,ds_list_find_value(global.clanNames,i));}